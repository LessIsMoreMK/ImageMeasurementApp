<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:ImageMeasurementApp">
    
    <Color x:Key="GrayDark">#373737</Color>
    <SolidColorBrush x:Key="GrayDarkBrush" Color="{StaticResource GrayDark}"/>
    <Color x:Key="GrayMedium">#575757</Color>
    <SolidColorBrush x:Key="GrayMediumBrush" Color="{StaticResource GrayMedium}"/>
    <Color x:Key="GrayLight">#808080</Color>
    <SolidColorBrush x:Key="GrayLightBrush" Color="{StaticResource GrayLight}"/>
    <Color x:Key="GrayVeryLight">#C0C0C0</Color>
    <SolidColorBrush x:Key="GrayVeryLightBrush" Color="{StaticResource GrayVeryLight}"/>
    <Color x:Key="AlmostWhite">#DBDBDB</Color>
    <SolidColorBrush x:Key="AlmostWhiteBrush" Color="{StaticResource AlmostWhite}"/>
    <Color x:Key="White">#FFFFFF</Color>
    <SolidColorBrush x:Key="WhiteBrush" Color="{StaticResource White}"/>

    <Color x:Key="BlueDark">#00445e</Color>
    <SolidColorBrush x:Key="BlueDarkBrush" Color="{StaticResource BlueDark}"/>
    <Color x:Key="BlueMedium">#00729e</Color>
    <SolidColorBrush x:Key="BlueMediumBrush" Color="{StaticResource BlueMedium}"/>
    <Color x:Key="BlueLight">#0099d4</Color>
    <SolidColorBrush x:Key="BlueLightBrush" Color="{StaticResource BlueLight}"/>

</ResourceDictionary>
